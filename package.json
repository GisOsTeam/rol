{
  "name": "@gisosteam/rol",
  "version": "0.1.9",
  "description": "OpenLayers with React",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --host 0.0.0.0 --port 3000 --config webpack.dev.config.js --content-base src --hot --inline",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'; prettier --single-quote --print-width 120 --write --parser typescript '{src,test}/**/*.tsx'",
    "lint": "npm run prettier && eslint . --ext .ts,.tsx",
    "genindex": "barrelsby -d ./src -D -L -l all -q -e sample -e __tests__ -e .d.ts",
    "build": "rimraf lib && npm run genindex && npm run lint && tsc -p tsconfig.json && cp ./package.json lib",
    "prepublishOnly": "[[ \"$PWD\" =~ /lib ]]",
    "pub": "pushd ./ && npm run build && cd lib && npm publish --access public && popd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GisOsTeam/rol.git"
  },
  "keywords": [
    "Web mapping",
    "React",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@types/jspdf": "^1.3.3",
    "@types/jszip": "^3.4.1",
    "@types/ol": "^6.5.3",
    "@types/react": "^18.0.18",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-loader": "^8.2.5",
    "barrelsby": "^2.4.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.8",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "ol": "6.5.0",
    "prettier": "^2.7.1",
    "proj4": "2.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@gisosteam/aol": "^0.2.7",
    "bhreq": "^0.1.2",
    "jspdf": "^2.5.1",
    "react-autosuggest": "^10.1.0",
    "react-draggable": "^4.4.5",
    "react-draggable-list": "^4.1.0",
    "react-icons": "^4.4.0",
    "styled-components": "^5.3.5"
  },
  "peerDependencies": {
    "ol": "^6.5.0",
    "proj4": "^2.8.0",
    "react": "^17.0.2"
  },
  "engines": {
    "node": ">= 10.22.1",
    "npm": ">= 6.14.6"
  }
}
