{
  "name": "@gisosteam/rol",
  "version": "0.0.22",
  "description": "OpenLayers with React",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --host 0.0.0.0 --port 3000 --config webpack.dev.config.js --content-base src --hot --inline",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'; prettier --single-quote --print-width 120 --write --parser typescript '{src,test}/**/*.tsx'",
    "lint": "npm run prettier && eslint . --ext .ts,.tsx",
    "genindex": "barrelsby -d ./src -D -L -l all -q -e sample -e __tests__ -e .d.ts",
    "build": "rimraf lib && npm run genindex && npm run lint && tsc -p tsconfig.json && cp ./package.json lib",
    "prepublishOnly": "[[ \"$PWD\" =~ /lib ]]",
    "pub": "pushd ./ && npm run build && cd lib && npm publish --access public && popd"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GisOsTeam/rol.git"
  },
  "keywords": [
    "Web mapping",
    "React",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/jszip": "^3.4.1",
    "@types/ol": "^6.3.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "barrelsby": "^2.2.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "ol": "6.3.1",
    "prettier": "^2.0.5",
    "proj4": "2.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "typescript": "3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@gisosteam/aol": "^0.0.44",
    "bhreq": "^0.1.0",
    "react-draggable": "^4.4.3",
    "react-draggable-list": "^4.0.3",
    "styled-components": "^5.1.1"
  },
  "peerDependencies": {
    "ol": "6.3.1",
    "proj4": "2.6.2",
    "react": "^16.13.1"
  },
  "engines": {
    "node": ">= 10.10.0",
    "npm": ">= 6.4.1"
  }
}
